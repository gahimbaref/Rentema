{
  "name": "rentema",
  "version": "1.0.0",
  "description": "Automated rental property management system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "verify": "node scripts/verify-setup.js",
    "migrate": "psql -U postgres -d rentema -f database/schema.sql",
    "migrate:email": "ts-node database/migrations/runner.ts",
    "migrate:email:run": "ts-node database/migrations/runner.ts run",
    "migrate:email:verify": "ts-node database/migrations/runner.ts verify",
    "migrate:email:all": "ts-node database/migrations/runner.ts run-and-verify"
  },
  "keywords": [
    "rental",
    "automation",
    "property-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "bull": "^4.11.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "fast-check": "^3.15.0",
    "googleapis": "^166.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.11.0",
    "redis": "^4.6.0"
  }
}
